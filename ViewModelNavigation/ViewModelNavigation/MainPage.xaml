<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:ViewModelNavigation"
             x:Class="ViewModelNavigation.MainPage"
             Title="Main page">

    <StackLayout>
        <Label Text="{Binding PageName}" VerticalOptions="Center" HorizontalOptions="Center" />
        <Button Text="Go to page 1" Command="{Binding OpenViewCommand}"/>
    </StackLayout>


</ContentPage>
